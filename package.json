{
  "name": "kodkafa-remix-template",
  "description": "this is a template to build seo oriented web projects by remix + tailwind + vite + typescript from KODKAFA",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "NODE_ENV=development node ./server.js",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "codegen": "pnpm openapi-ts openapi-config.ts",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@markdoc/markdoc": "^0.4.0",
    "@remix-run/express": "^2.9.2",
    "@remix-run/node": "^2.9.2",
    "@remix-run/react": "^2.9.2",
    "@unpic/react": "^0.1.14",
    "@vercel/analytics": "^1.3.1",
    "@vercel/remix": "2.9.2-patch.2",
    "address": "^2.0.2",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "i18next": "23.10.0",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-fs-backend": "^2.3.1",
    "i18next-http-backend": "^2.5.2",
    "isbot": "^4.1.0",
    "postcss": "^8.4.38",
    "prism-react-renderer": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "14.0.5",
    "remix-i18next": "6.0.1",
    "reading-time": "^1.5.0",
    "sharp": "^0.33.4"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.46.3",
    "@remix-run/dev": "^2.9.2",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.1.6",
    "vite": "link:@vercel/remix/vite",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}